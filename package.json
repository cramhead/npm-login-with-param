{
  "name": "npm-login-with-param",
  "version": "1.2.1",
  "description": "Login into npm from the non-interactive CLI",
  "bin": {
    "npm-login-with-param": "index.js"
  },
  "main": "index.js",
  "scripts": {
    "lint": "eslint -c .eslintrc.json .",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cramhead/npm-login-with-param.git"
  },
  "keywords": [
    "npm",
    "login",
    "cli"
  ],
  "author": "Marc d'Entremont",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cramhead/npm-login-with-param/issues"
  },
  "homepage": "https://github.com/arian/npm-login-with-param#readme",
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "npm-cache"
    ],
    "testPathIgnorePatterns": [
      "/util/"
    ],
    "silent": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "commander": "^8.1.0"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "jest-cli": "^27.0.6",
    "prettier": "^2.3.2"
  }
}
